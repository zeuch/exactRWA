Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"determine", " ", "effective", " ", "Hamiltonian"}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"effH", "[", "HH_", "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"MainMagnus", ",", "HRWA", ",", 
        RowBox[{"Heff", "=", "0"}]}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "first", " ", "determine", " ", "main", " ", "Magnus", " ", 
        "expansion"}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"verbose", "=", "True"}], ";"}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"MainMagnus", "=", 
        RowBox[{"M", "[", 
         RowBox[{"HH", ",", "max"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"verbose", "=", "False"}], ";"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"Print", "[", 
          RowBox[{"\"\<MainMagnus = \>\"", ",", "MainMagnus"}], "]"}], ";"}], 
        "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{"find", " ", "the", " ", "RWA"}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"Heff", "=", 
        RowBox[{"Coefficient", "[", 
         RowBox[{
          RowBox[{"MainMagnus", "/.", "reps"}], ",", 
          RowBox[{"1", "/", "ww"}], ",", "0"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Print", "[", 
        RowBox[{"\"\<HRWA = \>\"", ",", "Heff"}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"Print", "[", 
          RowBox[{"\"\<Besides, Coefficient[MainMagnus,1/ww] = \>\"", ",", 
           RowBox[{"Coefficient", "[", 
            RowBox[{"MainMagnus", ",", 
             RowBox[{"1", "/", "ww"}]}], "]"}]}], "]"}], ";"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"Print", "[", 
          RowBox[{"\"\<Besides, Coefficient[M[Heff,i],1/ww] = \>\"", ",", 
           RowBox[{"Coefficient", "[", 
            RowBox[{
             RowBox[{"M", "[", 
              RowBox[{"Heff", ",", "1"}], "]"}], ",", 
             RowBox[{"1", "/", "ww"}]}], "]"}]}], "]"}], ";"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"Print", "[", 
          RowBox[{"\"\<M[Heff,i] = \>\"", ",", 
           RowBox[{"M", "[", 
            RowBox[{"Heff", ",", "i"}], "]"}]}], "]"}], ";"}], "*)"}], 
       "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{"now", " ", "apply", " ", "iteration", " ", "procedure"}], 
        " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"iii", "=", "1"}], ",", 
         RowBox[{"iii", "\[LessEqual]", "max"}], ",", 
         RowBox[{"iii", "++"}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Print", "[", 
           RowBox[{
           "\"\<Running recursive procedure (iii = \>\"", ",", "iii", ",", 
            "\"\<)\>\""}], "]"}], ";", "\[IndentingNewLine]", 
          RowBox[{"Heff", "=", 
           RowBox[{
            RowBox[{"Heff", "+", 
             RowBox[{
              FractionBox["1", 
               SuperscriptBox["ww", "iii"]], 
              RowBox[{"Coefficient", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"MainMagnus", "-", 
                   RowBox[{"M", "[", 
                    RowBox[{"Heff", ",", "iii"}], "]"}]}], ")"}], "/.", 
                 "reps"}], ",", 
                RowBox[{"1", "/", "ww"}], ",", "iii"}], "]"}]}]}], "/.", 
            "repsInv"}]}], ";"}]}], "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
          RowBox[{"Print", "[", 
           RowBox[{"\"\<Heff = \>\"", ",", "Heff"}], "]"}], ";"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
          RowBox[{"Print", "[", 
           RowBox[{"\"\<iii = \>\"", ",", "iii"}], "]"}], ";"}], "*)"}], 
        "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"Print", "[", 
        RowBox[{"\"\<Finished @ \>\"", ",", " ", 
         RowBox[{
          RowBox[{"Date", "[", "]"}], "[", 
          RowBox[{"[", 
           RowBox[{"1", ";;", "5"}], "]"}], "]"}]}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Return", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Heff", "//", "Expand"}], "//", "S"}], "//", "TrigReduce"}],
         "]"}]}]}], "\[IndentingNewLine]", "]"}]}], ";"}]}]], "Input",
